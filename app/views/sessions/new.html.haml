.register.container-fluid
  .row
    .col-sm-10.col-sm-offset-1
      = bootstrap_form_tag url: sessions_path, layout: :horizontal, label_col: 'col-sm-2', control_col: 'col-sm-4' do |f|
        %header
          %h2 Sign in
        %fieldset
          = f.email_field :email, label: 'Email Address'
          = f.password_field :password, label: 'Password'
          = f.form_group do
            = f.submit 'Log in', class: 'btn btn-primary'